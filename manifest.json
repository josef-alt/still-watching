{
	"manifest_version": 3,
	"name": "still watching",
	"version": "2.0",
	"description": "Yes, I am still watching.",
	"icons": {
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"host_permissions": [
		"*://*.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["script.js"]
		}
	]
}
